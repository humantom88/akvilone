	<main class="admin__main">

		<div class="mt-5 mb-5">
			<a href="/admin/newProduct" class="btn btn-success">Добавить продукт</a>
		</div>

		{{#each products}}
			<div class="article product__admin">
				
				<a href="/admin/products/{{ this.link }}" class="admin__product_link">
					<img src="../img/productsTest/{{ this.imageUrl }}" alt="{{ this.name }}"
						class="product__image product__image_admin">
				</a>
				<div class="admin__product_controls">
					<a href="/admin/products/{{ this.link }}" class="article__title-link">
						<h3 class="article__title">{{ this.name }}</h3>
					</a>
					<div class="control-articles-wrapper">
						<div class="control-articles">
							<a href="/admin/products/edit/{{ this.id }}" class="btn btn-info">Редактировать</a>
							<form action="/admin/products/{{ this.id }}?_method=DELETE" method="POST">
								<button type="submit" class="btn btn-danger">Удалить</button>
							</form>
						</div>

						{{#if this.publicInProducts}} 
							<h3 class="article__published_check">опубликовано в продуктах</h3>
						{{/if}} 
						{{#if this.publicInReadySolutions}} 
							<h3 class="ready__published_check">опубликовано в готовых решениях</h3>
						{{/if}} 
					</div>
				</div>
			</div>
		{{/each}}

	</main>

